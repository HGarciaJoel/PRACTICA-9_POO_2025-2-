        -nombre: String
        -nivel: int
        -tipo: String
        -puntosDeVida: int
        -poderEspecial: String
        +atacar(): void
        +defenderse(): void
        +curarse(): void
        +huir(): void
    }

    class PokemonFuego
    class PokemonAgua
    class PokemonPlanta
    class PokemonElectrico
    class PokemonPsiquico
    class PokemonHielo
    class PokemonDragon
    class PokemonFantasma
